Config { font = "-misc-fixed-*-*-*-*-10-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , lowerOnStart = True
       , commands = [ Run Weather "KAUS" ["-t","Austin: <tempF>F","-L","50","-H","100","--normal","green","--high","red","--low","lightblue"] 36000
                    , Run Network "eth0" ["-L","0","-H","512","-m", "3", "--normal","green","--high","red"] 10
                    , Run Network "wlan0" ["-L","0","-H","512","-m", "3", "--normal","green","--high","red"] 10
                    , Run Cpu ["-L","3","-H","50","-p", "2", "--normal","green","--high","red"] 10
                    , Run BatteryP ["BAT0"]
                                   ["-L", "10", "-H", "80", "-p", "2",
                                    "--", "-O", "<fc=green>On</fc> - ", "-o", "",
                                    "-L", "-15", "-H", "-5",
                                    "-l", "red", "-m", "blue", "-h", "green"]
                                    600
                    , Run Date "%a %b %_d %Y %I:%M%p" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%cpu% | %eth0% - %wlan0% | %battery% } %StdinReader% { <fc=#ee9a00>%date%</fc> | %KAUS%"
       }

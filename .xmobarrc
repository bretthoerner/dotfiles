Config { font = "-misc-fixed-*-*-*-*-12-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , lowerOnStart = True
       , commands = [ Run Network "wlp4s0" ["-t", "<fc=green><rx>KB↓</fc> <fc=red><tx>KB↑</fc>"] 10
                    , Run Network "enp60s0u2u3" ["-t", "<fc=green><rx>KB↓</fc> <fc=red><tx>KB↑</fc>"] 10
                    , Run Cpu ["-t", "C: <total>%", "-L", "3", "-H", "50", "-p", "2", "--normal", "green", "--high", "red"] 10
                    , Run BatteryP ["BAT0", "BAT1"] ["-t", "B: <left>%", "-L", "10", "-H", "80", "-p", "3", "--", "-O", "<fc=green>On</fc> - ", "-o", "", "-L", "-15", "-H", "-5", "-l", "red", "-m", "blue", "-h", "green"] 600
                    , Run Date "%a %b %_d %Y %I:%M%p" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "<action=`gnome-system-monitor`>%cpu%</action> <fc=#ee9a00>|</fc> <action=`gnome-control-center network`>W: %wlp4s0% - E: %enp60s0u2u3% </action> } %StdinReader% { <action=`gnome-control-center power`>%battery%</action> <fc=#ee9a00>|</fc> %date%"
       }

Config { font = "-misc-fixed-*-*-*-*-10-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , lowerOnStart = True
       , commands = [ Run Network "enp0s25" ["-L","0","-H","512","-m", "3", "--normal","green","--high","red"] 10
                    , Run Network "wlp3s0" ["-L","0","-H","512","-m", "3", "--normal","green","--high","red"] 10
                    , Run Cpu ["-L","3","-H","50","-p", "2", "--normal","green","--high","red"] 10
                    , Run BatteryP ["BAT0"]
                                   ["-L", "10", "-H", "80", "-p", "2",
                                    "--", "-O", "<fc=green>On</fc> - ", "-o", "",
                                    "-L", "-15", "-H", "-5",
                                    "-l", "red", "-m", "blue", "-h", "green"]
                                    600
                    , Run Date "%a %b %_d %Y %I:%M%p" "date" 10
                    , Run StdinReader
                    , Run Com "~/bin/volume-info" [] "volume" 10
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%cpu% <fc=#ee9a00>|</fc> Net: %enp0s25% - %wlp3s0% <fc=#ee9a00>|</fc> %volume% } %StdinReader% { %battery% <fc=#ee9a00>|</fc> %date%"
       }
